{
  "name": "electron-kiva",
  "version": "1.2.10",
  "description": "Desktop application template",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev:build": "rimraf build & tsc & fuzhi ./public ./build & fuzhi ./package.json ./build/package.json",
    "dev:start": "npm run dev:build & electron ./build/main.js",
    "build": "npm run dev:build & cd build & electron-packager ./ kiva --app-version=1.2.10 --platform=win32 --arch=x64 --out='./out' --overwrite --download.mirrorOptions.mirror=https://npm.taobao.org/mirrors/electron/"
  },
  "author": "SystemLight",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "electron": "^11.2.3",
    "electron-packager": "^15.2.0",
    "eslint": "^7.11.0",
    "eslint-config-google": "^0.14.0",
    "fuzhi": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.1"
  }
}
